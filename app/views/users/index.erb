<h1>Users:</h1>
<ul><% @users.each do |user| %>
    <li><a href="/users/<%= user.id%>"><%= user.name %></a></li>

  <% end %></ul>

<% if logged_in? %>
  <h3>You are logged in as: <%= current_user.username %> </h3> <!--current_user method returns the user object -->
  <a href="/logout">Log out here</a>
<% end %>